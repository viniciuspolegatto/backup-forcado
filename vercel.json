{
  "version": 2,
  "rewrites": [
    { "source": "/enviar-email", "destination": "/api/enviar-email" }
  ],
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/cnpj/:cnpj",
      "dest": "server.js"
    },
    {
      "src": "(.*)",
      "dest": "server.js"
    }
  ]
}





